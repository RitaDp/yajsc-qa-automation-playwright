{
  "name": "yajsc-qa-automation-playwright",
  "version": "1.0.0",
  "description": "AQA test automation framework built using Playwright, Node.js, and JavaScript/TypeScript.",
  "main": "index.js",
  "scripts": {
    "test": "npx playwright test --project=full-run",
    "regression test": "npx playwright test --project=regression",
    "smoke test": "npx playwright test --project=smoke",
    "test:ui": "npx playwright test --project=full-run --ui",
    "report": "npx playwright show-report",
    "lint": "npx eslint .",
    "lint:fix": "npx eslint . --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RitaDp/yajsc-qa-automation-playwright.git"
  },
  "keywords": [],
  "author": "Marharyta Rovna",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/RitaDp/yajsc-qa-automation-playwright/issues"
  },
  "homepage": "https://github.com/RitaDp/yajsc-qa-automation-playwright#readme",
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.57.0",
    "@reportportal/agent-js-playwright": "^5.3.2",
    "@stylistic/eslint-plugin": "^5.6.1",
    "@testomatio/reporter": "^2.6.0",
    "@types/node": "^24.1.0",
    "eslint": "^9.39.2",
    "eslint-plugin-playwright": "^2.4.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.0"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "playwright-extra": "^4.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2"
  }
}
